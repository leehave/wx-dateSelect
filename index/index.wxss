}

.icon-portal-gps {
    display: inline-block;
    vertical-align: -6rpx;
    margin-right: 7rpx;
}

.icon-portal-loading {
    animation: iconLoading 1s linear infinite;
}

@keyframes iconLoading {
    0% {
        transform: rotate(0);
    }

    100% {
        transform: rotate(360deg);
    }
}

@-webkit-keyframes iconLoading {
    0% {
        transform: rotate(0);
    }

    100% {
        transform: rotate(360deg);
    }
}


    box-sizing: border-box;
}

[class^="icon-portal-"] {
    width: 34rpx;
    height: 32rpx;
    display: block;
}

.input-block {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    align-items: center;
    height: 100rpx;
    position: relative;
    background: #fff;
    border-radius: 4rpx;
}

.input-block.last::before {
    border: none;
}

.input-block::before {
    content: " ";
    position: absolute;
    bottom: 0;
    left: 4rpx;
    right: 4rpx;
    height: 0;
    border-bottom: 1px solid #e9e9e9;
    transform-origin: 0 100%;
    transform: scaleY(0.5);
}

.input-block__hd {
    margin-right: 26rpx;
}

.input-block__bd {
    -webkit-box-flex: 1;
    flex: 1 1 auto;
}

.btn-positioning {
    font-size: 22rpx;
